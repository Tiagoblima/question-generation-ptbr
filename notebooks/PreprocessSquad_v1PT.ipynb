        "patterns = [[{\"TEXT\": {\"REGEX\": \"<.*?>\"}},],\n",
        "[{\"TEXT\": {\"REGEX\": \"<\"}},\n",
        "{\"TEXT\": {\"REGEX\": \"h1>\"}}]\n",
        "]\n",
        "nlp = spacy.blank('pt')\n",
        "matcher = Matcher(nlp.vocab)\n",
        "matcher.add(\"delimiter_token\", patterns)"
