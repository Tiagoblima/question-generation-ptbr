        "    doc = nlp(example[\"paragraph_answer\"])\n",
        "    \n",
        "    example[\"sentence\"] = doc._.sentences[0].text\n",
        "    example[\"answer_sentence\"] = doc._.answer_sentence[0].text\n",
        "    example[\"paragraph_sentence\"] = doc._.paragraph_sentence[0].text\n",
        "    return example"
      ]
