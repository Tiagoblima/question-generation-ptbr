        "def get_sentence(token):\n",
        "    \"\"\"\n",
        "     Gets the sentence where the answer is found.\n",
        "    \"\"\"\n",
        "\n",
        "    for sent in token.doc.sents:\n",
        "        if sent.start <= token.i <= sent.end:\n",
        "            return sent"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
